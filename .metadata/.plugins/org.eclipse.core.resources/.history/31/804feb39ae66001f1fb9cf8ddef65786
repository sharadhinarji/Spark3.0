package com.ict.code;

public class stringEx {
	public static void main(String[] args) {
		String Ename="scott";
		int Eid=100;
		String EemailId="scott123@gmail.com";
		long Econtact=9019827163l;
		double Esal=30000;
		System.out.println("Employe Name:"+Ename);
		System.out.println("Employe id:"+Eid);
		System.out.println("Employe emailid:"+EemailId);
		System.out.println("Employee contact details:"+Econtact);
		System.out.println(Esal);
	}

}
